{
  "name": "@deeds-evidence/processing-system",
  "version": "2.0.0",
  "description": "GPU-accelerated evidence processing with Go microservices and LangExtract",
  "type": "module",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "microservices/*"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "capture:svelte-errors": "node scripts/capture-svelte-errors.mjs",
    "errors:patterns": "node scripts/extract-svelte-errors.mjs",
    "ci:svelte-gate": "node scripts/ci-svelte-gate.mjs",
    "check:auto:solve": "node scripts/check-auto-solve-extended.mjs",
    "solve:svelte:semantic": "node scripts/semantic-solve-svelte-errors.mjs",
    "solve:svelte:full": "npm run capture:svelte-errors && npm run errors:patterns && npm run solve:svelte:semantic",
    "solve:svelte:suggest": "npm run solve:svelte:full && npm run generate:suggestions && npm run apply:suggestions",
    "generate:suggestions": "node scripts/generate-suggestions.mjs",
    "generate:suggestions:concurrent": "node scripts/generate-suggestions-concurrent.mjs",
    "apply:suggestions": "node scripts/apply-suggestions.mjs",
    "solve:svelte:concurrent": "zx scripts/solve-svelte-concurrent.mjs",
    "cluster:orchestrate": "zx scripts/orchestrate-cluster.mjs",
    "dev": "pnpm -r run dev",
    "node-api:dev": "pnpm --filter @yorha/node-api run dev",
    "dev:full+nodeapi": "concurrently --restart-tries 3 --restart-after 4000 -n FRONTEND,CLUSTER,LB,MS,NODEAPI \"pnpm --filter yorha-legal-ai-frontend run dev\" \"pnpm run cluster:manager\" \"pnpm run lb:dev\" \"pnpm run microservices\" \"pnpm run node-api:dev\"",
    "dev:full": "concurrently --restart-tries 3 --restart-after 4000 -n FRONTEND,CLUSTER,LB,MS \"pnpm --filter yorha-legal-ai-frontend run dev\" \"pnpm run cluster:manager\" \"pnpm run lb:dev\" \"pnpm run microservices\"",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "test:frontend": "pnpm --filter sveltekit-frontend run test",
    "lint": "pnpm -r run lint",
    "setup": "node scripts/setup-optimized.mjs",
    "start": "node scripts/start-all.mjs",
    "stop": "node scripts/stop-all.mjs",
    "health": "node scripts/health-check.mjs",
    "detect": "node scripts/detect-services.mjs",
    "gpu-test": "node scripts/gpu-diagnostics.mjs",
    "microservices": "node scripts/start-microservices.mjs",
    "cluster:status": "node scripts/cluster-status.mjs",
    "ws:health": "powershell -NoProfile -Command \"Test-NetConnection -ComputerName localhost -Port 8080 | ConvertTo-Json\"",
    "bench:quic": "node scripts/quic-http-benchmark.mjs",
    "assets:validate": "node scripts/service-worker-asset-validator.mjs",
    "metrics:writer": "node scripts/metrics-writer.mjs",
    "demo": "node scripts/demo-optimized.mjs",
    "restoration:scan": "node scripts/restoration/cleanup-backups.mjs --dry-run",
    "restoration:apply": "node scripts/restoration/cleanup-backups.mjs --apply",
    "tasks:split": "node scripts/split-tasks.mjs",
    "tasks:merge": "node scripts/merge-tasks.mjs",
    "tasks:verify": "node scripts/verify-tasks-fragments.mjs",
    "prepare-hooks": "git config core.hooksPath .githooks",
    "cluster:manager": "node scripts/run-cluster-manager.mjs",
    "cluster:manager:debug": "LOG_LEVEL=debug node node-cluster/cluster-manager.cjs",
    "cluster:single": "node -e \"process.env.LOG_LEVEL='debug'; process.env.MANAGER_PORT_AUTO='1'; process.env.MANAGER_PORT='3099'; require('./node-cluster/cluster-manager.cjs')\" -- --manager-port=3099 --legal-count=1 --ai-count=1 --vector-count=1 --database-count=1",
    "db:retry": "node scripts/db-retry-wrapper.mjs",
    "lb:postcheck": "node scripts/lb-test.mjs",
    "lb:dev": "node scripts/run-load-balancer.mjs",
    "lb:test": "node scripts/test-load-balancer.mjs",
    "lb:status": "curl -s http://localhost:8099/status",
    "lb:prom": "curl -s http://localhost:8099/prometheus",
    "auto:solve": "node scripts/auto-solve.mjs",
    "auto:solve:watch": "nodemon -e js,mjs,ts --watch scripts --exec 'node scripts/auto-solve.mjs'",
    "vector:scan": "node scripts/scan-and-embed.mjs",
    "vector:search": "node scripts/search.mjs",
    "logs:ws": "node scripts/log-ws-server.mjs",
    "cli:detect": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/cli-detect-update.ps1",
    "cli:update": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/cli-detect-update.ps1 -AutoUpdate"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "@qdrant/js-client-rest": "^1.15.1",
    "amqplib": "^0.10.3",
    "archiver": "^7.0.1",
    "boxen": "^7.1.1",
    "bullmq": "^4.15.4",
    "chalk": "^5.3.0",
    "dotenv": "^17.2.1",
    "fastify": "^4.28.1",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.12",
    "ioredis": "^5.3.2",
    "minio": "^8.0.5",
    "nanospinner": "^1.1.0",
    "neo4j-driver": "^5.28.1",
    "ora": "^7.0.1",
    "p-limit": "^4.0.0",
    "p-retry": "^6.1.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.0",
    "pino": "^8.16.2",
    "systeminformation": "^5.21.20",
    "three": "^0.179.1",
    "xstate": "^5.20.2",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.33.1",
    "@types/archiver": "^6.0.3",
    "@types/minio": "^7.1.0",
    "@types/node": "^20.19.11",
    "@types/three": "^0.179.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.31.4",
    "nodemon": "^3.0.2",
    "svelte-check": "^3.8.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "resolutions": {
    "@typescript-eslint/utils": "^6.21.0",
    "magic-string": "^0.30.0",
    "vite": "^5.4.19",
    "rollup": "^4.0.0",
    "prettier": "^3.1.1"
  },
  "packageManager": "pnpm@8.15.1"
}
