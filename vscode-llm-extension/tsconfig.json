{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["ES2022"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "strict": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": false,
    "outDir": "./dist",
    "sourceMap": true
  },
  "include": [
    "src/extension.ts",
    "src/nomic-embed-service.ts",
    "src/neo4j-service.ts",
    "src/simd-json-parser.ts"
  ],
  "exclude": [
    "node_modules",
    "out",
    "dist",
    "../sveltekit-frontend",
    "../agents",
    "src/cluster-manager.ts",
    "src/enhanced-mcp-memory-manager.ts",
    "src/llm-optimization-manager.ts",
    "src/ollama-gemma-cache.ts"
  ]
}