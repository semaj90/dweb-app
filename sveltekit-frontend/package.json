{
  "name": "yorha-legal-ai-frontend",
  "version": "1.0.0",
  "description": "YoRHa-styled legal AI frontend with Gemma3 integration",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check:base": "node check-incremental.mjs",
    "post-check": "node post-check.js",
    "check": "npm run check:base && npm run post-check",
    "check:original": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:fast": "svelte-kit sync && svelte-check --threshold error --output human",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "ollama:start": "powershell -ExecutionPolicy Bypass -File ../scripts/start-ollama-service.ps1 start",
    "ollama:health": "powershell -ExecutionPolicy Bypass -File ../scripts/start-ollama-service.ps1 health",
    "ollama:models": "powershell -ExecutionPolicy Bypass -File ../scripts/start-ollama-service.ps1 models",
    "ollama:setup": "powershell -ExecutionPolicy Bypass -File ../scripts/start-ollama-service.ps1 setup",
    "ollama:gpu": "powershell -ExecutionPolicy Bypass -File ../scripts/start-ollama-service.ps1 start -GPU",
    "setup:local": "powershell -ExecutionPolicy Bypass -File ../scripts/setup-local-environment.ps1",
    "setup:postgres": "powershell -ExecutionPolicy Bypass -File ../scripts/setup-local-postgres.ps1",
    "dev:local": "powershell -ExecutionPolicy Bypass -File ../start-local-dev.ps1",
    "test:local": "powershell -ExecutionPolicy Bypass -File ../run-tests-local.ps1",
    "ai:test": "node ../scripts/test-ai-integration.mjs",
    "review-todos": "node scripts/review-todos.mjs",
    "test-ollama": "node scripts/test-ollama-integration.mjs",
    "memory-monitor": "node --max-old-space-size=4096 scripts/memory-monitor.mjs",
    "dev:memory": "NODE_OPTIONS='--max-old-space-size=4096' npm run dev",
    "build:memory": "NODE_OPTIONS='--max-old-space-size=8192' npm run build",
    "check:memory": "NODE_OPTIONS='--max-old-space-size=6144' npm run check",
    "dev:full": "concurrently \"npm run ollama:start\" \"npm run dev\" \"npm run db:studio\"",
    "context7:setup": "node ../scripts/context7-integration.js",
    "context7:legal-chat": "node ../scripts/legal-ai-chat.js",
    "context7:document-search": "node ../scripts/legal-document-search.js",
    "context7:mcp": "node ../SETUP-CLAUDE-MCP-CONTEXT7.bat",
    "cluster:start": "chmod +x scripts/start-cluster.sh && scripts/start-cluster.sh",
    "cluster:stop": "chmod +x scripts/stop-cluster.sh && scripts/stop-cluster.sh",
    "cluster:restart": "npm run cluster:stop && npm run cluster:start",
    "cluster:status": "chmod +x scripts/start-cluster.sh && scripts/start-cluster.sh status",
    "cluster:health": "chmod +x scripts/start-cluster.sh && scripts/start-cluster.sh health",
    "cluster:force-stop": "chmod +x scripts/stop-cluster.sh && scripts/stop-cluster.sh force",
    "cluster:clean": "chmod +x scripts/stop-cluster.sh && scripts/stop-cluster.sh clean",
    "cluster:dev": "node cluster.js",
    "cluster:scale-up": "kill -USR1 $(cat cluster.pid 2>/dev/null || echo '')",
    "cluster:scale-down": "kill -USR2 $(cat cluster.pid 2>/dev/null || echo '')",
    "cluster:reload": "kill -HUP $(cat cluster.pid 2>/dev/null || echo '')",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:ollama": "playwright test ollama-integration.spec.ts",
    "test:rag": "playwright test rag-system.spec.ts",
    "test:db": "playwright test postgresql-pgvector.spec.ts drizzle-orm.spec.ts",
    "test:pipeline": "playwright test rag-pipeline-integration.spec.ts",
    "test:gpu": "playwright test gpu-acceleration.spec.ts",
    "test:all": "powershell -ExecutionPolicy Bypass -File tests/run-all-tests.ps1",
    "test:auth": "playwright test user-authentication-session.spec.ts",
    "test:store": "playwright test global-store-state.spec.ts",
    "test:health": "playwright test system-health-logging.spec.ts",
    "test:sw": "playwright test service-worker.spec.ts",
    "test:xstate": "playwright test xstate-machines.spec.ts",
    "test:loki": "playwright test lokijs-caching.spec.ts",
    "test:ingestion": "playwright test gpu-ollama-ingestion.spec.ts",
    "test:comprehensive": "playwright test user-authentication-session.spec.ts global-store-state.spec.ts system-health-logging.spec.ts service-worker.spec.ts xstate-machines.spec.ts lokijs-caching.spec.ts gpu-ollama-ingestion.spec.ts",
    "test:quick": "playwright test quick-validation.spec.ts",
    "test:full-stack": "powershell -ExecutionPolicy Bypass -File tests/start-ollama-and-test.ps1",
    "test:report": "playwright show-report"
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.2.20",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.26.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/amqplib": "^0.10.7",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.29.1",
    "drizzle-orm": "^0.44.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^25.0.1",
    "mode-watcher": "^1.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.14.2",
    "svelte-check": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^6.0.1",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@iconify-json/lucide": "^1.2.58",
    "@iconify-json/mdi": "^1.2.3",
    "@langchain/community": "^0.3.49",
    "@langchain/core": "^0.3.66",
    "@langchain/ollama": "^0.2.3",
    "@langchain/openai": "^0.6.3",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@melt-ui/svelte": "^0.86.6",
    "@modelcontextprotocol/sdk": "*",
    "@node-rs/argon2": "^2.0.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@qdrant/js-client-rest": "^1.15.0",
    "@tailwindcss/typography": "^0.5.10",
    "@tauri-apps/api": "^2.7.0",
    "@tiptap/core": "^3.0.7",
    "@tiptap/extension-collaboration": "^3.0.7",
    "@tiptap/extension-focus": "^3.0.7",
    "@tiptap/extension-image": "^3.0.7",
    "@tiptap/extension-link": "^3.0.7",
    "@tiptap/extension-placeholder": "^3.0.7",
    "@tiptap/extension-text-align": "^3.0.7",
    "@tiptap/starter-kit": "^3.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/pdfkit": "^0.17.2",
    "@unocss/vite": "^66.3.3",
    "@webgpu/types": "^0.1.64",
    "@xstate/svelte": "^5.0.0",
    "amqplib": "^0.10.8",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bits-ui": "^2.8.13",
    "cssnano": "^7.1.0",
    "date-fns": "^4.1.0",
    "fabric": "^5.3.0",
    "fuse.js": "^7.1.0",
    "gl-matrix": "^3.4.3",
    "happy-dom": "^18.0.1",
    "idb-keyval": "^6.2.2",
    "ioredis": "^5.6.1",
    "langchain": "^0.3.30",
    "lokijs": "^1.5.12",
    "lucia": "^3.2.2",
    "lucide-svelte": "^0.474.0",
    "marked": "^16.1.1",
    "pdf2pic": "^3.2.0",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "pgvector": "^0.1.8",
    "postcss-preset-env": "^10.2.4",
    "postgres": "^3.4.3",
    "redis": "^5.7.0",
    "rxdb": "^16.16.0",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.3",
    "svelte-dnd-action": "^0.9.64",
    "sveltekit-superforms": "^2.27.1",
    "tailwind-merge": "^2.2.0",
    "tailwind-variants": "^0.1.18",
    "tesseract.js": "^6.0.1",
    "unocss": "^66.3.3",
    "xss": "^1.0.15",
    "xstate": "^5.20.1"
  }
}
