        "dev:realtime": "concurrently \"npm run websocket:start\" \"npm run dev\"",
        "websocket:start": "node websocket-server.js",
        "websocket:dev": "nodemon websocket-server.js",
        "db:setup": "node setup-database.mjs",
        "db:reset": "drizzle-kit drop && drizzle-kit push",
        "db:seed": "node setup-database.mjs --seed",
        "db:fix": "FIX-DATABASE-SCHEMA.bat",
        "db:start-docker": "cd ../.. && powershell -ExecutionPolicy Bypass -File start-database.ps1",
        "build": "vite build",
        "build:prod": "NODE_ENV=production vite build",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
