    "p-retry": "^6.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.6",
    "rimraf": "^5.0.10",
    "svelte": "^5.14.2",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.5.0",
