    "autoprefixer": "^10.4.16",
    "boxen": "^7.1.1",
    "chalk": "^5.5.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.29.1",
    "drizzle-orm": "^0.44.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "glob": "^11.0.3",
