# Development Environment Configuration
NODE_ENV=development

# Database Configuration
DATABASE_URL=postgresql://legal_admin:123456@localhost:5432/legal_ai_db
PGPASSWORD=123456

# AI Services Configuration
OLLAMA_URL=http://localhost:11434
QDRANT_URL=http://localhost:6333

VITE_GO_GPU_URL=http://localhost:8084
GO_GPU_SERVICE_URL=http://localhost:8084
DB_HOST=localhost
DB_PORT=5432
DB_NAME=legal_ai_db
DB_USER=postgres
DB_PASSWORD=123456
GO_MICROSERVICE_URL=http://localhost:8084
GO_SERVER_URL=http://localhost:8084
OLLAMA_API_URL=http://localhost:11434

# Use existing Ollama models and caches
OLLAMA_MODEL=gemma3-legal:latest
EMBEDDING_MODEL=nomic-embed-text:latest

# MCP Multi-cluster configuration
MCP_SERVER_HOST=localhost:40000
MCP_WORKER_THREADS=4

# Feature Flags
VITE_GPU_ENABLED=true
VITE_DEMO_MODE=true
VITE_UPLOAD_MAX_SIZE=50000000

# Cache optimization - use existing downloads
