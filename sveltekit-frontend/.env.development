HF_HOME=%USERPROFILE%\.cache\huggingface
TRANSFORMERS_CACHE=%USERPROFILE%\.cache\transformers

# PostgreSQL Configuration for Legal AI System
DATABASE_URL=postgresql://legal_admin:123456@localhost:5432/legal_ai_db

# Alternative format if needed
DB_HOST=localhost
DB_PORT=5432
DB_NAME=legal_ai_db
DB_USER=legal_admin
DB_PASSWORD=123456

# GPU Processing Configuration
GPU_ENABLED=true
EMBEDDING_DIM=768
MAX_DOCUMENTS=10000
BATCH_SIZE=512

# Redis Configuration
REDIS_URL=redis://localhost:6379

# API Configuration
API_PORT=8080
API_HOST=localhost

# Frontend Configuration
PUBLIC_API_URL=http://localhost:8080
PUBLIC_API_BASE_URL=http://localhost:8080/api
PUBLIC_GO_BACKEND_URL=http://localhost:8080
VITE_API_URL=http://localhost:8080/api

# Development Flags
SKIP_RAG_INITIALIZATION=true
SKIP_QDRANT_HEALTH_CHECK=true
