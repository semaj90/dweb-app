  "error_pipeline": {
    "watch_files": [
      "svelte-errors.json",
      "build-errors.json", 
      "type-errors.json"
    ],
    "parallel_processing": true,
    "batch_size": 8,
    "embedding_model": "nomic-embed-text",
    "vector_dimensions": 384
  }
}